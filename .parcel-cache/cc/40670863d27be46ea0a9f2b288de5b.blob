le {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 1rem;\r\n}\r\n\r\nh1 {\r\n  font-family: 'Redressed', sans-serif;\r\n  font-size: 2.4rem;\r\n  font-weight: lighter;\r\n}\r\n\r\n.logo i {\r\n  font-size: 200%;\r\n}\r\n\r\n.search-form {\r\n  display: flex;\r\n  align-items: stretch;\r\n}\r\n\r\n.search-form input {\r\n  font-family: 'Roboto', sans-serif;\r\n  padding: .75rem 1rem;\r\n  border-top-left-radius: .5rem;\r\n  border-bottom-left-radius: .5rem;\r\n  border: none;\r\n}\r\n\r\n.search-form input:focus {\r\n  outline: none;\r\n}\r\n\r\n.search-form button {\r\n  border: none;\r\n  display: flex;\r\n  align-items: center;\r\n  padding: .5rem 1rem;\r\n  border-top-right-radius: .5rem;\r\n  border-bottom-right-radius: .5rem;\r\n  gap: 1rem;\r\n  background:#2F1B41;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n\r\n.header .bookmarks {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 1rem;\r\n}\r\n\r\n.header .bookmarks h5 {\r\n  font-size: 1.6rem;\r\n  font-weight: 500;\r\n  text-transform: uppercase;\r\n}\r\n\r\n\r\n/* Main */\r\n\r\n.main {\r\n  display: flex;\r\n  min-height: 60vh;\r\n}\r\n\r\n\r\n/* Main Left */\r\n\r\n.main-left {\r\n  flex: 1 0 1rem;\r\n  background: #f4f4f4;\r\n  padding: 3rem 0;\r\n  position: relative;\r\n  border-bottom-left-radius: 10px;\r\n}\r\n\r\n.users-preview {\r\n  list-style: none;\r\n}\r\n\r\n.user-preview {\r\n  text-decoration: none;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 2rem;\r\n  font-size: 2.5rem;\r\n  padding: 1.5rem 2rem;\r\n  color: #9F1E49;\r\n  transition: all .5s ease-out;\r\n}\r\n\r\n.user-preview-item:not(:last-child) .user-preview {\r\n  border-bottom: 1px solid #9F1E49;\r\n}\r\n\r\n.user-preview:hover {\r\n  background:#9F1E49;\r\n  color: #fff;\r\n}\r\n\r\n.user-preview img {\r\n  border-radius: 50%;\r\n  width: 7rem;\r\n  height: 7rem;\r\n}\r\n\r\n.pagination-buttons {\r\n  display: flex;\r\n  gap: 1.5rem;\r\n  justify-content: center;\r\n  margin-top: 3rem;\r\n}\r\n\r\n.pagination-buttons button {\r\n  border: none;\r\n  background: #9F1E49;\r\n  color: #fff;\r\n  padding: 1rem 2rem;\r\n  border-radius: 1rem;\r\n  cursor: pointer;\r\n  transition: all .5s ease-out;\r\n  display: flex;\r\n  gap: 1rem;\r\n  align-items: center;\r\n  font-weight: 700;\r\n}\r\n\r\n.pagination-buttons button.page-prev {\r\n  margin-right: auto;\r\n  margin-left: 1rem;\r\n}\r\n\r\n.pagination-buttons button.page-next {\r\n  margin-left: auto;\r\n  margin-right: 1rem;\r\n}\r\n\r\n\r\n.pagination-buttons button i {\r\n  transition: transform 0.25s ease-in-out;\r\n}\r\n\r\n.pagination-buttons button.page-prev:hover i {\r\n  transform: translateX(-.5rem);\r\n}\r\n\r\n.pagination-buttons button.page-next:hover i {\r\n  transform: translateX(.5rem);\r\n}\r\n\r\n\r\n/* Main Right */\r\n\r\n.main-right {\r\n  flex: 3;\r\n  background: #fff;\r\n  padding: 3rem;\r\n  border-bottom-right-radius: 10px;\r\n}\r\n\r\n.main-right .main-top {\r\n  display: flex;\r\n  gap: 2rem;\r\n  position: relative;\r\n}\r\n\r\n.main-top {\r\n  min-height: 20vh;\r\n}\r\n\r\n.main-image {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  text-align: center;\r\n}\r\n\r\n.main-image img {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 10px;\r\n}\r\n\r\n.main-content {\r\n  flex: 2;\r\n}\r\n\r\n.main-name {\r\n  font-size: 2.5rem;\r\n  color:#9F1E49;\r\n}\r\n\r\n.main-description {\r\n  font-size: 1.5rem;\r\n}\r\n\r\n.user-info {\r\n  list-style: none;\r\n  margin-top: 1rem;\r\n}\r\n\r\n.user-info i {\r\n  color: #9F1E49;\r\n}\r\n\r\n.user-info li {\r\n  font-size: 1.6rem;\r\n  font-weight: 400;\r\n}\r\n\r\n.user-info li span {\r\n  font-weight: 500;\r\n}\r\n\r\n.user-info li:not(:last-child) {\r\n  margin-bottom: .5rem;\r\n}\r\n\r\n.stats {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  font-size: 2rem;\r\n  margin-top: 2.5rem;\r\n}\r\n\r\n.stats .stat {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex: 1;\r\