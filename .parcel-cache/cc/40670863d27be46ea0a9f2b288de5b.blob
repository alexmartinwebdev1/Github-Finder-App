display: flex;\r\n  flex-direction: column;\r\n  text-align: center;\r\n}\r\n\r\n.main-image img {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 10px;\r\n}\r\n\r\n.main-content {\r\n  flex: 2;\r\n}\r\n\r\n.main-name {\r\n  font-size: 2.5rem;\r\n  color:#9F1E49;\r\n}\r\n\r\n.main-description {\r\n  font-size: 1.5rem;\r\n}\r\n\r\n.user-info {\r\n  list-style: none;\r\n  margin-top: 1rem;\r\n}\r\n\r\n.user-info i {\r\n  color: #9F1E49;\r\n}\r\n\r\n.user-info li {\r\n  font-size: 1.6rem;\r\n  font-weight: 400;\r\n}\r\n\r\n.user-info li span {\r\n  font-weight: 500;\r\n}\r\n\r\n.user-info li:not(:last-child) {\r\n  margin-bottom: .5rem;\r\n}\r\n\r\n.stats {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  font-size: 2rem;\r\n  margin-top: 2.5rem;\r\n}\r\n\r\n.stats .stat {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex: 1;\r\n  gap: 2rem;\r\n  padding: 2rem;\r\n  transition: all .5s ease-out;\r\n}\r\n\r\n.stats .stat:not(:last-child) {\r\n  border-right: 1.25px solid #9F1E49;\r\n}\r\n\r\n.stats .stat:hover {\r\n  background: #9F1E49;\r\n  color: #fff;\r\n}\r\n\r\n.stat h6 {\r\n  line-height: 1;\r\n  font-size: 1.3rem;\r\n}\r\n\r\n.stat-name-number p {\r\n  font-weight: 700;\r\n  color: #9F1E49;\r\n  transition: color .5s ease-out;\r\n}\r\n\r\n.stat:hover .stat-name-number p {\r\n  color: #fff;\r\n}\r\n\r\n.go-to-profile {\r\n  text-decoration: none;\r\n  font-size: 1.6rem;\r\n  border: 2px solid #9F1E49;\r\n  border-radius: 5px;\r\n  padding: 1rem 2rem;\r\n  display: inline-block;\r\n  color: #9F1E49;\r\n  transition: all .5s ease-out;\r\n  margin-top: 2rem\r\n}\r\n\r\n.go-to-profile:hover {\r\n  background: #9F1E49;\r\n  color: #fff;\r\n}\r\n\r\n\r\n/* Main Bottom - Repos */\r\n\r\n.main-bottom {\r\n  margin-top: 4rem;\r\n  color: #fff;\r\n}\r\n\r\n.main-bottom a {\r\n  text-decoration: none;\r\n  color: #fff;\r\n}\r\n\r\n.main-bottom h3 {\r\n  font-size: 1.9rem;\r\n  color: #9F1E49;\r\n  margin-bottom: 1.2rem;\r\n}\r\n\r\n.main-bottom .repos {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n  position: relative;\r\n}\r\n\r\n.main-bottom .repo {\r\n  background: #9F1E49;\r\n  padding: 1.5rem 3rem;\r\n  transition: transform .2s ease-out;\r\n}\r\n\r\n.main-bottom .repo:hover {\r\n  transform: translateY(-6px);\r\n}\r\n\r\n.repo h6 {\r\n  font-size: 1.7rem;\r\n  color: #fff;\r\n  font-weight: 500;\r\n}\r\n\r\n.repo-desc {\r\n  font-size: 1.5rem;\r\n  font-weight: 400;\r\n  color: #fff;\r\n}\r\n\r\n.repo-stats {\r\n  display: flex;\r\n  gap: 1rem;\r\n  margin-top: .5rem;\r\n}\r\n\r\n.error {\r\n  text-align: center;\r\n  color: #9F1E49;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  font-size: 2rem;\r\n  padding: 2rem;\r\n  width: 100%;\r\n}\r\n\r\n/* Spinner */\r\n\r\n.spinner {\r\n  width: 5rem;\r\n  height: 5rem;\r\n  font-size: 5rem;\r\n  animation: spin 1s linear infinite;\r\n  transform-origin: center;\r\n  color: #9F1E49;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n@keyframes spin {\r\n  from {\r\n    transform: rotate(0deg);\r\n  }\r\n  to {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 1000px) {\r\n  .main {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .main-left {\r\n    border-bottom-left-radius: 0;\r\n  }\r\n\r\n  .main-right {\r\n    border-bottom-left-radius: 10px;\r\n  }\r\n\r\n  .pagination-buttons button.page-prev {\r\n    margin-left: 5rem;\r\n  }\r\n\r\n  .pagination-buttons button.page-next {\r\n    margin-right: 5rem;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 768px) {\r\n  .main-top {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .main-top img {\r\n    width: 50%;\r\n    margin: auto\r\n  }\r\n\r\n  .logo-and-title {\r\n    flex: 2;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n  .header {\r\n    flex-direction: column;\r\n    gap: 1.5rem;\r\n    padding-bottom: 3rem !important;\r\n  }\r\n\r\n  .logo-and-title {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n\r\n  .logo-and-title, .search-form {\r\n    width: 80%;\r\n  }\r\n\r\n  .pagination-buttons button.page-prev {\r\n    margin-left: 2rem;\r\n  }\r\n\r\n  .pagination-buttons button.page-next {\r\n    margin-right: 2rem;\r\n  }\r\n\r\n  .stats {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .stats .stat {\r\n    flex-direction: column;\r\n    gap: 0rem;\r\n  }\r\n\r\n  .stats .stat:not(:last-child) {\r\n    border-bottom: none !important;\r\n   }\r\n\r\n  .stats .stat {\r\n    border: 1.5px solid #9F1E49 !important;\r\n  }\r\n\r\n  .stat-name-number {\r\n    text-align: center;\r\n  }\r\n}\n/*# sourceMappingURL=style.HASH_REF_e7b65a8fbba0bd60f56116c660a10f35.css.map */\n"],"names":[],"version":3,"file":"style.HASH_REF_e7b65a8fbba0bd60f56116c660a10f35.css.map"}